<%= turbo_stream_from current_store, "electronics" %>
<main class="content w-full">
<div class="header flex justify-between w-full">
    <h1 class="text-4xl">Shop for Electronics at <%= current_store.name %></h1>
    <%= link_to "New Electronic", 
    new_electronic_path, 
    data: {turbo_frame: dom_id(Electronic.new)},
    #  data: {turbo_frame: "_top"},
    class: "text-white bg-pink-400 rounded-lg px-6 py-4" %>
</div>

<%= turbo_frame_tag Electronic.new %>
<%= turbo_frame_tag "electronics" do %>
    <%= render @electronics %>
<% end %>
</main>